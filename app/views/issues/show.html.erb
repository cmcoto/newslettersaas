



<h3><%= @issue.title %> </h3>
<%= link_to 'Edit', edit_issue_path(@issue) %> |
<%= link_to 'Back', issues_path %>


<ul>
  <% @issue.links.each do |link| %>
  	<div class="callout">
  		<h4><%= link.title %></h4>
  
  		<p><%= link.content %></p>
   		<a href= "<%= link.url %>"><%= link.url %></a>
	</div>
  <% end %>
</ul>
